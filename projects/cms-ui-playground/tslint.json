{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      false,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      false,
      "element",
      "lib",
      "kebab-case"
    ],
    "variable-name": false,
    "import-blacklist": [
      true,
      ["^@cms-ui\\/core\\/.*"]
    ]
  },
  "linterOptions": {
    "exclude": [
      ".*\\/index$"
    ]
  }
}
