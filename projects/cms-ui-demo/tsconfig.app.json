/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@cms-ui/core": [
        "../cms-ui/src/public-api.ts"
      ],
      "@cms-ui/core/*": [
        "../cms-ui/src/public-api.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": true
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
